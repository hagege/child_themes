/* ----------------------------- */
/* Navigation                    */
/* ----------------------------- */

@media (min-width: 769px) {
    .main-navigation .menu > .menu-item > a::after
	{
        content: "";
        position: absolute;
        right: 0;
        /* left: 50%; */
        bottom: -5px;
        -webkit-transform: translateX(-50%);
        transform: translateX(-50%);
        display: block;
        width: 0;
        height: 5px;
				color: #622523;
 background-color: currentColor;
        transition: 0.3s width ease; 
    }
    .main-navigation .menu > .menu-item.current-menu-item > a::after,
    .main-navigation .menu > .menu-item.current-menu-ancestor > a::after,
    .main-navigation .menu > .menu-item > a:hover::after {
        width: 50%;
    }
}

/* graue Linie zwischen Menü und Content */ 
.separate-containers .site-main {
    margin: 0px;
    border-top: 5px solid #9a9a9a;
}

/* Hover bei Untermenüpunkten */
.main-navigation .main-nav ul ul li[class*="current-menu-"] > a:hover, .main-navigation .main-nav ul ul li[class*="current-menu-"].sfHover > a {
    color: #eaeaea;
    background-color: #622523;
}


/* Button unten auf den Seiten */
a.oben.__mPS2id._mPS2id-h {
    background: url(http://test.dev-haurand.de/wp-content/uploads/2020/12/Button_blau_hoch.png) no-repeat;
		padding-bottom: 60px;
    margin-top: 120px; 
}

/* hover bei Button unten */
a.oben.__mPS2id._mPS2id-h:hover {
    background: url(http://test.dev-haurand.de/wp-content/uploads/2020/12/Button_rot_rauf.png) no-repeat;
		padding-bottom: 60px;
    margin-top: 120px; 
}

/* damit image button unten nicht gezeigt wird */
img.wp-image-519 {
    visibility: hidden;
}

/* ----------------------------- */
/* Text auf Bildern je nach device anpassen */
/* ----------------------------- */


.fadeInOut_3 {
    opacity: 0;
    -webkit-animation: fadein_3 15s; /* Safari, Chrome and Opera > 12.1 */
    -moz-animation: fadein_3 15s; /* Firefox < 16 */
    -ms-animation: fadein_3 15s; /* Internet Explorer */
    -o-animation: fadein_3 15s; /* Opera < 12.1 */
    animation: fadein_3 15s;
}

 @keyframes fadein_3 {
    0% { opacity: 0; }
    80%   { opacity: 1; }
    100% {opacity: 0;}
}


/* Textzeile für Device (ganz oben) */
@media only screen and (min-width: 551px) {
	p.has-text-align-center.fadeInOut_3.has-large-font-size {
			font-size: 65px;
		}
}
@media only screen and (min-width: 401px) and (max-width: 550px) {
	p.has-text-align-center.fadeInOut_3.has-large-font-size {
			font-size: 35px;
		}
}
@media only screen and (max-width: 400px) {
	p.has-text-align-center.fadeInOut_3.has-large-font-size {
			font-size: 25px;
		}
}


/* Textzeile für die beiden unteren Schriften auf dem Bild */
@media only screen and (min-width: 551px) {
	p.has-text-align-left.animated.bounceInRight.delay-3s {
			font-size: 65px;
		}
}
@media only screen and (min-width: 401px) and (max-width: 550px) {
	p.has-text-align-left.animated.bounceInRight.delay-3s {
			font-size: 35px;
		}
}
@media only screen and (max-width: 400px) {
	p.has-text-align-left.animated.bounceInRight.delay-3s {
			font-size: 25px;
		}
}


