		"css": ":where(.wp-site-blocks *:focus){outline-width:2px;outline-style:solid}\n\n/* Rand zwischen Header und Content */ \n:where(.wp-site-blocks) > * {\n    margin-block-start: 0rem;\n}\n\n\n/* Bildunterschriften zentriert */\nfigure.wp-block-image figcaption {\n    text-align: center;\n}\n\n\n/* Button: hover \n.wp-element-button:hover, .wp-block-button__link:hover {\n    background-color: var(--wp--preset--color--contrast-3);\n    border-color: var(--wp--preset--color--contrast-3);\n    color: var(--wp--preset--color--base);\n}\n*/\n\na.wp-block-navigation-item__content {\n    text-underline-offset: 7px;\n    transition: all .2s;\n    text-decoration: underline;\n    text-decoration-thickness: 1px;\n}\n\n\na.wp-block-navigation-item__content:hover {\n    text-underline-offset: 10px;\n    text-decoration-thickness: 2px;\n}\n\n\na.wp-block-navigation-item__content:focus{\n    outline: 1px dotted;\n}\n\n\n@media (min-width: 600px) {\n.wp-block-navigation__responsive-container:not(.hidden-by-default):not(.is-menu-open) {\ndisplay: none;\n}\n\n.wp-block-navigation__responsive-container:not(.hidden-by-default):not(.is-menu-open) .wp-block-navigation__responsive-container-close {\n    display: none\n}\n\n.wp-block-navigation__responsive-container.is-menu-open .wp-block-navigation__submenu-container.wp-block-navigation__submenu-container.wp-block-navigation__submenu-container.wp-block-navigation__submenu-container {\n    left: 0\n}\n}\n\n@media (min-width: 600px) and (max-width: 800px) {\n.wp-block-navigation__responsive-container-open:not(.always-shown) {\ndisplay: flex;\n}\n}\n\n@media (min-width: 801px) {\n.wp-block-navigation__responsive-container:not(.hidden-by-default):not(.is-menu-open) {\ndisplay: block;\n}\n\n.wp-block-navigation__responsive-container:not(.hidden-by-default):not(.is-menu-open) .wp-block-navigation__responsive-container-close {\n    display: none\n}\n\n.wp-block-navigation__responsive-container.is-menu-open .wp-block-navigation__submenu-container.wp-block-navigation__submenu-container.wp-block-navigation__submenu-container.wp-block-navigation__submenu-container {\n    left: 0\n}\n}",
