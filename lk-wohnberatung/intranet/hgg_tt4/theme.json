		"css": ":where(.wp-site-blocks *:focus){outline-width:2px;outline-style:solid}\n\n/* für die Spalten in den Kacheln */\n.kachel-spalte {\n    border-radius: 15px;\n}\n\n\n/* Gestaltung der Kategorienliste */\n\nul.kategorie-liste {\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: center;\n}\n\nul.kategorie-liste >li a {\n    display: flex;\n    padding: 10px 15px;\n    margin: 5px;\n    background-color: var(--wp--preset--color--base-2);\n    color: var(--wp--preset--color--contrast-2);\n    text-decoration: none;\n    border-radius: 5px;\n    transition: background-color 0.3s;\n}\n\nul.kategorie-liste >li a:hover {\n    background-color: var(--wp--preset--color--contrast-4);\n}\n\n/* Animation in der Navigation */\na.wp-block-navigation-item__content {\n    transition: all 1.5s;\n}\n\n\na.wp-block-navigation-item__content:hover {\n    transition: all 1.5s;\n}           \n\n:where(.wp-site-blocks *:focus){outline-width:2px;outline-style:solid}\na.wp-block-navigation-item__content {\n    transition: all 1.5s;\n}\n\n\na.wp-block-navigation-item__content:hover {\n    transition: all 1.5s;\n}           \n\n:where(.wp-site-blocks *:focus){outline-width:2px;outline-style:solid}\na.wp-block-navigation-item__content {\n    transition: all 1.5s;\n}\n\n\na.wp-block-navigation-item__content:hover {\n    transition: all 1.5s;\n}           \n\n:where(.wp-site-blocks *:focus){outline-width:2px;outline-style:solid}\na.wp-block-navigation-item__content {\n    transition: all 1.5s;\n}\n\n\na.wp-block-navigation-item__content:hover {\n    transition: all 1.5s;\n} \n\n\n\n/* Unterstrich grün */\n:root :where(.wp-block-navigation a:where(:not(.wp-element-button)):hover) {\n    border-bottom-color: var(--wp--preset--color--base-2);\n    border-bottom-width: 4px;\n    border-bottom-style: solid;\n    text-decoration: none;\n}",
